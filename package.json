    "axios": "^0.19.2",
    "axios-retry": "^3.1.8",
    "balloon-css": "^1.2.0",
    "bulma": "^0.8.2",
    "clean-webpack-plugin": "^3.0.0",
    "date-fns": "^2.15.0",
    "dompurify": "^2.0.12",
    "domurl": "^2.3.4",
    "file-saver": "^2.0.2",
    "handsontable": "^7.4.2",
    "jquery": "^3.5.1",
    "jszip": "^3.5.0",
    "jszip-utils": "^0.1.0",
    "lodash": "^4.17.19",
    "retry-axios": "^2.2.1",
